<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<fx:root prefHeight="400.0" prefWidth="700.0" type="BorderPane" xmlns="http://javafx.com/javafx/20" xmlns:fx="http://javafx.com/fxml/1">
    <center>
        <VBox>
            <TitledPane fx:id="qp" animated="false" collapsible="true" maxHeight="1.7976931348623157E308" text="QuPath extensions" VBox.vgrow="ALWAYS">
                <ListView fx:id="extensionListView1" maxHeight="1.7976931348623157E308" styleClass="extension-manager-list-view" />
            </TitledPane>
            <TitledPane fx:id="other" animated="false" collapsible="true" maxHeight="1.7976931348623157E308" text="Other extensions" VBox.vgrow="ALWAYS">
                <ListView fx:id="extensionListView2" maxHeight="1.7976931348623157E308" styleClass="extension-manager-list-view" />
            </TitledPane>
        </VBox>
    </center>
    <bottom>
        <AnchorPane fx:id="ap">
            <HBox AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0">
                <Button fx:id="openExtensionDirBtn" onAction="#openExtensionDir" text="%ExtensionControlPane.openExtensionDirectory" HBox.hgrow="ALWAYS" maxWidth="Infinity">
                    <tooltip>
                        <Tooltip text="%ExtensionControlPane.openExtensionDirectoryTooltip" />
                    </tooltip>
                </Button>
                <Button fx:id="mangeIndexesBtn" onAction="#openExtensionDir" text="%ExtensionControlPane.manageExtensionIndexes" HBox.hgrow="ALWAYS" maxWidth="Infinity">
                    <tooltip>
                        <Tooltip text="%ExtensionControlPane.manageExtensionIndexesTooltip" />
                    </tooltip>
                </Button>
            </HBox>
        </AnchorPane>
    </bottom>
</fx:root>
